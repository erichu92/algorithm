#include <iostream>
#include <stdio.h>
using namespace std;

int main() {

	int n; // number of inputs
	
	cin >> n; // get number of inputs
	
	int* stack = new int[n]; // stack allocation
	int top = 0; // top of the stack
	
	for (int i = 0; i < n; i++) {
		stack[i] = 0;
	}

	char* output = new char[2 * n]; // output array
	int index = 0; // index of output array
	
	int max = 0;

	for (int i = 0; i < n; i++) { // get inputs
		int k;
		cin >> k;

		if (max <= k) {
			int iter_num = k - max;
			for (int j = 0; j < iter_num; j++) { // push
				stack[top++] = ++max;
				output[index++] = '+';
			}

			{
				stack[--top] = 0; // pop
				output[index++] = '-';
			}
		}

		else {
			if (stack[top - 1] == k) {
				stack[--top] = 0;
				output[index++] = '-';
			}
			else {
				printf("NO\n");
				return 0;
			}
		}
	}

	for (int i = 0; i < 2*n; i++) {
		printf("%c\n", output[i]);
	}
}
